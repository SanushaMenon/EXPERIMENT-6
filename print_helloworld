#include<iostream>
using namespace std;

int main() {
    int i;
    for(i=0;i<10;i++) {
        cout<<"Hello World ("<<i+1<<")"<<endl;
    }

/*Output

Hello World (1)
Hello World (2)
Hello World (3)
Hello World (4)
Hello World (5)
Hello World (6)
Hello World (7)
Hello World (8)
Hello World (9)
Hello World (10) */

}
